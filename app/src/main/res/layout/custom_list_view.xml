<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CommonListingFragment">

    <!-- TODO: Update blank fragment layout -->


    <ImageView
        android:id="@+id/list_view_image"
        android:layout_width="68dp"
        android:layout_height="54dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="11dp"
        android:src="@android:drawable/btn_star_big_on" />

    <TextView
        android:id="@+id/list_view_title"
        android:layout_width="297dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/list_view_image"
        android:layout_marginLeft="100dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/list_view_description"
        android:layout_width="297dp"
        android:layout_height="34dp"
        android:layout_alignBottom="@+id/list_view_image"
        android:layout_alignLeft="@+id/list_view_title"
        android:text="TextView" />
</RelativeLayout>